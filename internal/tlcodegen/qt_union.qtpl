        {%s= field.t.ReadTL2Call(directImports, bytesVersion, "currentR", fmt.Sprintf("item.value%s", field.goName), false, union.wr.ins, field.recursive) %}
        {%- endif -%}
    {%- endfor -%}
    }
    {%- endif -%}
    {%s= union.wr.gen.InternalPrefix()%}Unused(currentR)
    return r, nil
}
