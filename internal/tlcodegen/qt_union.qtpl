func (item *{%s= goName %}) InternalWriteTL2(w []byte, sizes []int, optimizeEmpty bool) ([]byte, []int, int) {
    {%- if !union.IsEnum -%} {% code /* copy of logic from struct.qtpl */ %}
    switch item.index {
    {%- for i, field := range union.Fields -%}
    {%- if !field.t.IsTrueType() -%}
